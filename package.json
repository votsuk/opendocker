{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "0.3.0",
  "name": "opendocker",
  "type": "module",
  "private": false,
  "bin": {
    "opendocker": "./bin/opendocker"
  },
  "files": ["bin/", "package.json", "README.md"],
  "scripts": {
    "dev": "bun run --conditions=browser ./src/index.tsx",
    "build": "bun run scripts/build.ts",
    "build:single": "bun run scripts/build.ts --single",
    "binary": "bun run scripts/build.ts --single"
  },
  "devDependencies": {
    "@tsconfig/bun": "latest",
    "@types/bun": "latest"
  },
  "dependencies": {
    "@opentui/core": "^0.1.69",
    "@opentui/solid": "^0.1.69",
    "@types/dockerode": "^3.3.47",
    "clipboardy": "^5.0.2",
    "dockerode": "^4.0.9",
    "solid-js": "^1.9.10"
  }
}
